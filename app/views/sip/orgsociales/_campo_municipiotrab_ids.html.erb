<% estmostrar = 'display: none' %>
<% if f.object.tipoorg_id == 1 || f.object.tipoorg_id == 2 %>
  <% estmostrar = '' %>
<% end %>
<%= render partial: 'sip/modelos/sip_campo_aut', 
  locals: {f: f, atr: :municipiotrab, 
           htmlexterno: {class: 'estorganizacion', style: estmostrar} 
  } %>


<%= f.input :region, 
  disabled: true,
  include_blank: true,
  collection: Sivel2Gen::Region.all,
  selected: f.object.region_id,
  label_method: :presenta_nombre,
  input_html: {
  },
  wrapper_html: {
    class: 'estorganizacion',
    style: estmostrar
  }
%>
